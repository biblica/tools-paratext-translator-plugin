<?xml version="1.0"?>
<CheckAndFixItem xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Id>a4a9e6df-51f0-421f-acda-91237649f0e2</Id>
  <Name>In Text (ignores markers and headings)</Name>
  <Description>Finds text and ignores markers and headings.</Description>
  <Version>1.0.0</Version>
  <Scope>CHAPTER</Scope>
  <DefaultItemDescription>Value Found</DefaultItemDescription>
  <CheckRegex>(?&lt;=((\\\+?(i[^de]\w*|[smlpq]\w*|r|d|nb|cl|s|tr|tc\w+|v\s+\S+|f[^r]\w*|x[^o]\w*|add|bk|tl|sc|nd|wj?|rq)[\*\s])|(f|x|fe|ef|c|fr|xo)\s+\S\s*(?=\\)|\\(f|x|fe|ef)\*))[^\\]*?(?=\\|\||\Z):::</CheckRegex>
  <FixRegex></FixRegex>
  <CheckScript></CheckScript>
</CheckAndFixItem>
